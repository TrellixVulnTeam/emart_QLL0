uB,IAAvB,EAAuB,IAAvB,EAAuB,gBAAA,CAAhB,IAAI,CAAC,GAAW,CAAvB,EAAe;AAAA;AAAf,a,EACA,YAAA,CAAgC,IAAhC,EAAgC,IAAhC,EAAgC,gBAAA,CAAzB,IAAI,CAAC,YAAoB,CAAhC,EAAwB;AAAA;AAAxB,a,EACA,YAAA,CAA0B,IAA1B,EAA0B,IAA1B,EAA0B,gBAAA,CAAnB,IAAI,CAAC,MAAc,CAA1B,EAAkB;AAAA;AAAlB,a,EACA,YAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,IAAI,CAAC,OAAe,CAA3B,EAAmB;AAAA;AAAnB,a,EACA,YAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,IAAI,CAAC,OAAe,CAA3B,EAAmB;AAAA;AAAnB,a,EAEA,YAAA,CAmBK,IAnBL,EAmBK,IAnBL,EAmBK,CAlBH,YAAA,CAMc,sBANd,EAMc;AANA,cAAA,EAAE,EAAA;AAAA,gBAAA,IAAA,EAAA,aAAA;AAAA,gBAAA,MAAA,EAAA;AAAA,kBAAA,EAAA,EAAoC,IAAI,CAAC;AAAzC;AAAA;AAMF,aANd,E;kCACE;AAAA,uBAIS,CAJT,WAIS,CAJT;AAAA,e;;;;aADF,E;;AAAA,c,MAAA,CAkBG,EAVH,YAAA,CAMS,QANT,EAMS;AAND,cAAA,IAAI,EAAC,QAMJ;AALD,uBAAM,oEAKL;AAJA,cAAA,OAAK,EAAA,cAAA,CAAA,UAAA,MAAA;AAAA,uBAAU,IAAA,CAAA,OAAA,CAAQ,IAAI,CAAC,GAAb,CAAV;AAAA,eAAA,EAA0B,CAAA,SAAA,CAA1B;AAIL,aANT,E,CAIE,W,CAJF,E;;AAAA,c,WAAA,CAUG,CAnBL,C,CAfF,C;WAmCK,CAnCL,E;;AAAA,WAAuC,EAAnC;AAAA,SADU,CAuCV;;;;AAAA,OA3DR,CA6DI,CA9DN,C,IA+DgB,KAAA,CAAA,MAAA,KAAM,G,kBAAtB,YAAA,CAEM,KAFN,EAAA,WAAA,EAAiE,kBAAjE,C,qCAjEO,CAtCP;AAAA,K;;;;GALF,C","sourcesContent":["<template>\n  <card\n    class=\"no-border-card\"\n    body-classes=\"px-0 pb-1 py-3\"\n    footer-classes=\"pb-2\"\n  >\n    <form @submit.prevent=\"fetch\">\n      <div class=\"row px-4 pb-3\">\n        <label class=\"mt-2 text-dark\">Filter:</label>\n        <div class=\"col-2 pr-0\">\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            placeholder=\"Product Name\"\n            aria-controls=\"datatables\"\n            v-model=\"form.productName\"\n          />\n        </div>\n        <div class=\"col-2 pr-0\">\n          <input\n            type=\"search\"\n            class=\"form-control\"\n            placeholder=\"EAN Code\"\n            aria-controls=\"datatables\"\n            v-model=\"form.EANCode\"\n          />\n        </div>\n        <div class=\"col-2 pr-0\">\n          <input\n            type=\"search\"\n            class=\"form-control\"\n            placeholder=\"SKU Code\"\n            aria-controls=\"datatables\"\n            v-model=\"form.SKUCode\"\n          />\n        </div>\n        <div class=\"col-1\">\n          <base-button type=\"default\" native-type=\"submit\">Search</base-button>\n        </div>\n        <div class=\"col-1\">\n          <button type=\"button\" class=\"btn base-button btn-default\" @click.prevent=\"fetchProduct\">Reset</button>\n        </div>\n\n      </div>\n    </form>\n\n    <div v-if=\"visibleProductDetails\">\n      <Table>\n        <template #thead>\n          <tr>\n            <th>Sr No</th>\n            <th>ID</th>\n            <th>EAN Code</th>\n            <th>HSN Code</th>\n            <th>Brand Name</th>\n            <th>Mftr Article No/SKU Code</th>\n            <th>Product Category</th>\n            <th>Product Name</th>\n            <th>MRP</th>\n            <th>Selling Price</th>\n            <th>Margin</th>\n            <th>Product Scheme</th>\n            <th>Remarks</th>\n\n          </tr>\n\n        </template>\n        <template #tbody>\n          <tr v-for=\"(data,index) in productData\" :key=\"data._id\">\n            <td>{{index+1}}</td>\n            <td>{{data._id}}</td>\n            <td>{{ data.EANCode }}</td>\n            <td>{{ data.HSNCode }}</td>\n            <td>{{ data.brandName }}</td>\n            <td>{{ data.SKUCode }}</td>\n            <td>{{ data.productCategory }}</td>\n            <td>{{ data.productName }}</td>\n            <td>{{ data.MRP }}</td>\n            <td>{{ data.sellingPrice }}</td>\n            <td>{{ data.margin }}</td>\n            <td>{{ data.schemes }}</td>\n            <td>{{ data.remarks }}</td>\n\n            <td>\n              <router-link :to=\"{name:'SchemesEdit',params: { id: data._id}}\">\n                <button type=\"button\"\n                        class=\"btn base-button btn-icon btn-fab btn base-button btn-default btn-sm edit\">\n                  <i class=\"text-white ni ni-ruler-pencil\"></i>\n\n                </button>\n              </router-link>\n\n              <button type=\"button\"\n                      class=\"btn base-button btn-icon btn-fab btn-danger btn-sm remove btn-link\"\n                      @click.prevent=\"destroy(data._id)\"\n              >\n                <i class=\"text-white ni ni-fat-remove\"></i>\n\n              </button>\n\n\n\n            </td>\n          </tr>\n\n        </template>\n      </Table>\n\n    </div>\n    <div v-else-if=\"status===201\" class=\"text-center mt-4 text-dark\">\n      Data not found\n    </div>\n    <template v-slot:footer>\n      <div\n        class=\"col-12 d-flex justify-content-center justify-content-sm-between flex-wrap\"\n      >\n        <div class=\"\">\n          <p class=\"card-category\">\n            Showing {{ from + 1 }} to {{ to }} of {{ total }} entries\n          </p>\n        </div>\n        <base-pagination\n          class=\"pagination-no-border\"\n          v-model=\"pagination.currentPage\"\n          :per-page=\"pagination.perPage\"\n          :total=\"total\"\n        >\n        </base-pagination>\n      </div>\n    </template>\n  </card>\n\n</template>\n<script>\n\nimport BasePagination from \"@/components/BasePagination\";\nimport Card from \"../../Cards/Card\";\nimport axios from \"axios\";\n\nexport default {\n  components: {\n    Card,\n    BasePagination,\n  },\n\n  data() {\n    return {\n      form:{\n        productName:'',\n        EANCode:'',\n        SKUCode:'',\n      },\n\n      pagination: {\n        perPage: 10,\n        currentPage: 1,\n        perPageOptions: [5, 10, 25, 50],\n        total: 0,\n      },\n\n      productData:[],\n      visibleProductDetails:false,\n      status:'',\n    };\n  },\n  mounted() {\n    this.fetchProduct();\n  },\n  methods: {\n\n    fetchProduct(){\n      axios.get(`https://vuecrud78.herokuapp.com/api/product/get`)\n        .then(response=>{\n          this.productData=response.data;\n          console.log(this.productData);\n          this.status=response.status;\n          console.log(this.productData);\n          if(this.status==200){\n            this.visibleProductDetails=true;\n          }\n          else if(this.status==201){\n            this.visibleProductDetails=false;\n          }\n        });\n      this.form={};\n    },\n\n    fetch(){\n      axios.get(`https://vuecrud78.herokuapp.com/api/product/get?`,{\n        params: {\n          productName: this.form.productName,\n          EANCode: this.form.EANCode,\n          SKUCode: this.form.SKUCode\n        }\n      })\n      .then(response=>{\n        this.productData=response.data;\n        this.status=response.status;\n        console.log(this.productData);\n        if(this.status==200){\n          this.visibleProductDetails=true;\n        }\n        else if(this.status==201){\n          this.visibleProductDetails=false;\n        }\n      });\n    }\n\n  },\n};\n</script>\n<style>\n.no-border-card .card-footer {\n  border-top: 0;\n}\n</style>\n"],"sourceRoot":""}]}