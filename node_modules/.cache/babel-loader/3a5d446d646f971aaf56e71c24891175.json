ile\"\n      class=\"custom-file-input\"\n      id=\"customFileLang\"\n      lang=\"en\"\n      v-bind=\"$attrs\"\n      v-on=\"listeners\"\n    />\n    <label class=\"custom-file-label\" for=\"customFileLang\">\n      {{ label }}\n    </label>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"file-input\",\n  inheritAttrs: false,\n  props: {\n    initialLabel: {\n      type: String,\n      default: \"Select file\",\n    },\n  },\n  data() {\n    return {\n      files: [],\n    };\n  },\n  computed: {\n    listeners() {\n      return {\n        change: this.fileChange,\n      };\n    },\n    label() {\n      let fileNames = [];\n      for (let file of this.files) {\n        fileNames.push(file.name);\n      }\n      return fileNames.length ? fileNames.join(\", \") : this.initialLabel;\n    },\n  },\n  methods: {\n    fileChange(evt) {\n      this.files = evt.target.files;\n      this.$emit(\"change\", this.files);\n    },\n  },\n};\n</script>\n<style></style>\n"],"sourceRoot":""}]}