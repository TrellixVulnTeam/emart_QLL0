EL,UAAA,OAAO,EAAE,KAAK,IAAL,CAAU,OAFd;AAGL,UAAA,QAAQ,EAAC,KAAK,IAAL,CAAU;AAHd;AADyB,OAAlC,EAOC,IAPD,CAOM,UAAA,QAAQ,EAAE;AACd,QAAA,KAAI,CAAC,kBAAL,GAAwB,QAAQ,CAAC,IAAjC;AACA,QAAA,KAAI,CAAC,MAAL,GAAY,QAAQ,CAAC,MAArB;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,MAArB;;AACA,YAAG,KAAI,CAAC,MAAL,IAAa,GAAhB,EAAoB;AAClB,UAAA,KAAI,CAAC,OAAL,GAAa,IAAb;AACF,SAFA,MAGK,IAAG,KAAI,CAAC,MAAL,IAAa,GAAhB,EAAoB;AACvB,UAAA,KAAI,CAAC,OAAL,GAAa,KAAb;AACF;AACD,OAjBD;AAkBD,KApBM;AAqBP,IAAA,SArBO,uBAqBI;AACT,WAAK,IAAL,GAAU,EAAV;AACA,WAAK,mBAAL;AACF;AAxBO;AA3BI,CAAf","sourcesContent":["<template>\n\n        <card\n          class=\"no-border-card\"\n          body-classes=\"px-0 pb-1 py-3\"\n          footer-classes=\"pb-2\"\n        >\n          <form>\n            <div class=\"row px-2\">\n              <div class=\"col-3 pr-0 form-group\">\n                <div class=\"d-flex\">\n                  <label class=\"mt-2 px-1\">From:</label>\n                  <input type=\"date\" class=\"form-control\" placeholder=\"from\" v-model=\"form.startDate\">\n                </div>\n              </div>\n              <div class=\"col-3 pr-0 form-group\">\n                <div class=\"d-flex\">\n                  <label class=\"mt-2 px-1\">To:</label>\n                  <input type=\"date\" class=\"form-control\" placeholder=\"to\" v-model=\"form.endDate\">\n                </div>\n              </div>\n              <div class=\"col-2 pr-0\">\n                <input\n                  type=\"text\"\n                  placeholder=\"PO Number\"\n                  class=\"form-control\"\n                  v-model=\"form.PONumber\"\n                />\n              </div>\n              <div class=\"col-1 pr-0\">\n                <button\n                  class=\"btn base-button btn-default\"\n                  type=\"button\"\n                  @click=\"fetchPurchaseOrders\"\n                >\n                  search\n                </button>\n              </div>\n              <div class=\"col-1 pl-4\">\n                <button\n                  class=\"btn base-button btn-default\"\n                  type=\"button\"\n                  @click=\"resetForm\"\n                >\n                  reset\n                </button>\n              </div>\n            </div>\n          </form>\n          <div class=\"pl-0\" v-if=\"visible\">\n            <Table>\n              <template #thead>\n                <tr>\n                  <th>Sr No</th>\n                  <th>ID</th>\n                  <th>PO Number</th>\n                  <th>Date</th>\n                  <th>No. of Items</th>\n                  <th>Value</th>\n                  <th>Status</th>\n                  <th>Payment received</th>\n                  <th>Delivery Status</th>\n                </tr>\n\n              </template>\n              <template #tbody>\n                <tr v-for=\"(data,index) in purchaseOrdersData\" :key=\"data._id\">\n                  <td>{{index+1}}</td>\n                  <td>{{data._id}}</td>\n                  <td>{{ data.PONumber }}</td>\n                  <td>{{ data.Date }}</td>\n                  <td>{{ data.NoOfItems }}</td>\n                  <td>{{ data.value }}</td>\n                  <td>{{ data.status }}</td>\n                  <td>{{ data.paymentReceived }}</td>\n                  <td>{{ data.deliveryStatus }}</td>\n                </tr>\n\n              </template>\n            </Table>\n          </div>\n          <div v-else-if=\"status===201\" class=\"text-center mt-4 text-dark\">\n            Data not found\n          </div>\n          <template v-slot:footer>\n            <div\n              class=\"col-12 d-flex justify-content-center justify-content-sm-between flex-wrap\"\n            >\n              <div class=\"\">\n                <p class=\"card-category\">\n                  Showing {{ from + 1 }} to {{ to }} of {{ total }} entries\n                </p>\n              </div>\n              <base-pagination\n                class=\"pagination-no-border\"\n                v-model=\"pagination.currentPage\"\n                :per-page=\"pagination.perPage\"\n                :total=\"total\"\n              >\n              </base-pagination>\n            </div>\n          </template>\n        </card>\n\n</template>\n<script>\n\nimport BasePagination from \"@/components/BasePagination\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BasePagination,\n  },\n  data() {\n    return {\n      form:{\n        startDate:'',\n        endDate:'',\n        PONumber:'',\n      },\n      purchaseOrdersData: [],\n      visible:false,\n      error:'',\n      status:'',\n      pagination: {\n        perPage: 10,\n        currentPage: 1,\n        perPageOptions: [5, 10, 25, 50],\n        total: 0,\n      },\n\n    };\n  },\n  mounted(){\n    this.fetchPurchaseOrders();\n  },\n  methods: {\n    fetchPurchaseOrders(){\n      axios.get(`api/purChaseOrder/get`,{\n        params:{\n          startDate: this.form.startDate,\n          endDate: this.form.endDate,\n          PONumber:this.form.PONumber\n        }\n      })\n      .then(response=>{\n        this.purchaseOrdersData=response.data;\n        this.status=response.status;\n        console.log(response.status);\n        if(this.status==200){\n          this.visible=true;\n        }\n        else if(this.status==201){\n          this.visible=false;\n        }\n      });\n    },\n    resetForm(){\n      this.form={};\n      this.fetchPurchaseOrders();\n    }\n  },\n};\n</script>\n<style>\n.no-border-card .card-footer {\n  border-top: 0;\n}\n</style>\n"],"sourceRoot":""}]}