n\n            setEndDate(date) {\n                this.$emit('update:end_date', date);\n\n                if (this.period === 'CUSTOM') {\n                    this.$emit('input');\n                }\n            }\n        },\n\n        computed: {\n            periodLabel() {\n                return _.get(this.options, this.period);\n            },\n\n            customPeriod() {\n                return this.period === 'CUSTOM';\n            }\n        }\n    }\n</script>\n\n\n<style scoped>\n\n</style>\n"]}]}